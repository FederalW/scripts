local KeyCode = getgenv().camshake.keybind
getgenv().camshake = getgenv().camshake.enabled 
getgenv().lolza = false
getgenv().shake_cam_speed = getgenv().camshake.speed
getgenv().shake_cam_value = getgenv().camshake.pixels

function BB()
  x = math.random(-getgenv().shake_cam_value,getgenv().shake_cam_value)
y = math.random(-getgenv().shake_cam_value,getgenv().shake_cam_value)
mousemoverel(x,y)
end

game:GetService('UserInputService').InputBegan:Connect(function(Key)
    if getgenv().camshake == true and Key.KeyCode == Enum.KeyCode[KeyCode:upper()] and not game:GetService('UserInputService'):GetFocusedTextBox() then
        if getgenv().lolza then
            getgenv().lolza = false

        elseif not getgenv().shake_cam then
            getgenv().lolza = true

            while getgenv().lolza do
                BB()
                task.wait(getgenv().shake_cam_speed)
            end
        end
    end
end)
